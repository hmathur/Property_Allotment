<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<constant name="struts.devMode" value="true" />

	<package name="default" namespace="/" extends="struts-default">
		<action name="UserLogin" class="action.UserLogin" method="execute">
			<result name="success">/Welcome.jsp</result>
			<result name="input">/Login.jsp</result>

		</action>
		<action name="home" class="action.Action" method="execute">
			<result name="success">/Welcome.jsp</result>
			<result name="input">/Login.jsp</result>

		</action>
		<action name="ulogin" method="execute" class="action.UserRegis">

			<result name="success">/display.jsp</result>

			<result name="input">/Register.jsp</result>

		</action>
	</package>
</struts>      